{
  "table": {
    "name": "$PROJECT-cdn-$ENV_TYPE",
    "available_regions": [
      "us-east-1"
    ]
  },
  "config_table": {
    "name": "$PROJECT-config-$ENV_TYPE",
    "available_regions": [
      "us-east-1"
    ]
  },
  "config_cache_ttl": 2,
  "headers": {
    "max_age": "600"
  },
  "logging": {
    "version": 1,
    "incremental": "True",
    "disable_existing_loggers": "False",
    "formatters": {
      "default": {
        "format": "%(asctime)s - %(levelname)s - %(aws_request_id)s - %(message)s\n",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
    },
    "loggers": {
      "origin-response": {
        "level": "WARNING"
      },
      "origin-request": {
        "level": "WARNING"
      },
      "default": {
        "level": "WARNING"
      }
    }
  }
}
